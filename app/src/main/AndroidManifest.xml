<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jansanjivani">

    <uses-permission
        android:name="android.permission.ACCESS_COARSE_LOCATION"
        android:maxSdkVersion="22" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JanSanjivani">
        <activity android:name=".admin_password"></activity>
        <activity android:name=".App_Admins" />
        <activity android:name=".Bloods_display" />
        <activity android:name=".NewUserBloodBank" />
        <activity android:name=".ForgotPasswordBloodBank" />
        <activity android:name=".SigninBloodBank" />
        <activity android:name=".BloodBankclass" />
        <activity android:name=".SimpleUserclass" />
        <activity android:name=".Logins" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>